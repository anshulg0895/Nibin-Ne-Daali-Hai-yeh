<!DOCTYPE html>
<html>
  <head>

  </head>
  <body>
    <h1>Welcome Page</h1>

    <%= form_for :student, url: {:controller=>'students', :action=>'index'} do |f| %>

      <p>
        <%= f.label :Email  %><br>
        <%= f.text_field :email %>
      </p>

      <p>
        <%= f.label :Password  %><br>
        <%= f.text_field :password %>
      </p>

      <p>
        <%= f.submit :LogIn %>
      </p>

    <% end %>
  </body>
</html>
