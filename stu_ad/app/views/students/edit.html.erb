<!DOCTYPE html>
<html>
  <head>
  </head>
  <body>
    <h1>Edit Student Detail</h1>
    <%= form_for @student, as: :student, url: student_path(@student), method: :patch do |f| %>
      <p>
        <%= f.label :Name %><br>
        <%= f.text_field :name %>
      </p>
      <p>
        <%= f.label :Contact_No %><br>
        <%= f.text_field :contact %>
      </p>
      <p>
        <%= f.label :Email_Id %><br>
        <%= f.text_field :email %>
      </p>
      <p>
        <%= f.label :Father_Name %><br>
        <%= f.text_field :father %>
      </p>
      <p>
        <%= f.label :Age %><br>
        <%= f.text_field :age %>
      </p>
      <p>
        <%= f.submit :Update %>
      </p>
    <% end %>
  </body>
</html>
